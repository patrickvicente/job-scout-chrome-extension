{
    "compensation": {
      "currencies": [
        {
          "symbol": "$",
          "code": "USD",
          "floor": 20e3,
          "ceiling": 1e6
        },
        {
          "symbol": "£",
          "code": "GBP",
          "floor": 20e3,
          "ceiling": 1e6
        },
        {
          "symbol": "€",
          "code": "EUR",
          "floor": 20e3,
          "ceiling": 1e6
        },
        {
          "symbol": "CA$",
          "code": "CAD",
          "floor": 30e3,
          "ceiling": 1.5e6
        },
        {
          "symbol": "AU$",
          "code": "AUD",
          "floor": 30e3,
          "ceiling": 1.5e6
        }
      ],
      "currencyPatterns": ["({SYMBOL}|{CODE})"],
      "currencyScore": 15,
      "currencyScorePatterns": [
        "{CURRENCY_PATTERN}\\s*{NUMBER}",
        "{NUMBER}\\s*{CURRENCY_PATTERN}"
      ],
      "multipliers": {
        "\\s*(k(\\W+|$)|thousand)": 1e3,
        "\\s+000": 1e3,
        "\\s*(m(\\W+|$)|million)": 1e6,
        "\\s*MM(\\W+|$)": 1e6,
        "\\s*(b(\\W+|$)|billion)": 1e9,
        "\\s*(t(\\W+|$)|trillion)": 1e12
      },
      "numContextWords": 7,
      "numberIndexPattern": "\\d[\\d,.]+",
      "numberPattern": "([\\d,]+(\\.\\d+)?)({MULTIPLIERS})?",
      "periods": [
        ["year", "y\\W", "yr\\W", "annual", "annually"],
        ["month", "m\\W", "mth\\W"],
        ["week", "w\\W", "wk\\W"],
        ["hour", "h\\W", "hr\\W"]
      ],
      "periodsInYear": {
        "hour": 2000,
        "week": 50,
        "month": 12,
        "year": 1,
        "": 1
      },
      "periodPatterns": [
        "(^|\\W+){PERIOD}ly(\\W+|$)",
        "(^|\\W+)(an?|per|/)\\s+({SHORTS})(\\W+|$)",
        "(^|\\W+)({SHORTS})(\\W+|$)"
      ],
      "periodScore": 2,
      "replaces": {
        "job\\s*id:\\s*\\S+": "",
        "hour\\s+shift": ""
      },
      "scoreThreshold": 10,
      "scores": {
        "between\\W+{NUMBER}\\W+([&]|and)\\W+{NUMBER}": 7,
        "{NUMBER}\\s*(to|[—–-]+|[&]|and)\\s*{NUMBER}": 7,
        "start(s|ing)\\s+at\\W+{NUMBER}": 7,
        "(^|\\W+)(salary|compensation|pay|rate)(\\W+|$)": 5,
        "{NUMBER}\\s*\\%": -5,
        "\\W*((time|days)\\s+off|holidays)\\W*": -5,
        "countries": -5,
        "education": -5,
        "employees": -5,
        "experience": -5,
        "languages": -5,
        "linkedin est\\.": -5,
        "match": -5,
        "sign\\s*on": -5,
        "years?\\s*of": -5,
        "years?\\s+old": -5,
        "401\\s*\\(?k\\)?": -7,
        "(lbs|kg)": -10
      }
    },
    "maintenance": {
      "job": "",
      "contact": "",
      "company": ""
    },
    "mutationObservers": {
      "monster.com/jobs/": "[class*=JobContainer]",
      "glassdoor.(com|sg|ca|co.uk)/Job/": "[class*=pageContainer]"
    },
    "ignore": [],
    "jobAppLikeness": {
      "maxDistance": {
        "left": 32,
        "top": 32
      },
      "threshold": 15,
      "knownUrls": [
        ".*\\.breezy\\.hr/p/.*/apply",
        "app\\.dover\\.io/apply/.*/.*",
        "apply\\.workable\\.com/.*/j/.*/apply",
        "boards\\.greenhouse\\.io/.*/jobs/.*",
        "careers\\.hireology\\.com/.*/.*/description",
        "jobs\\.jobvite\\.com/.*/job/.*",
        "jobs\\.smartrecruiters\\.com/oneclick-ui/company/.*/publication/.*",
        ".*\\.bamboohr\\.com/jobs/view.php\\?id=.*",
        "dynamitejobs\\.com/apply/.*"
      ],
      "preventOn": ["linkedin\\.com/jobs"],
      "matchConfigs": {
        "goodHeading": {
          "score": 10,
          "tags": "h1,h2,h3,h4,h5,h6",
          "matches": [
            "apply for this (?:job|position)",
            "submit(?:\\syour)? application"
          ]
        },
        "okayHeading": {
          "score": 5,
          "tags": "h1,h2,h3,h4,h5,h6",
          "matches": ["$\\s*apply\\s*^"]
        },
        "contactFields": {
          "score": 5,
          "tags": "label",
          "matches": ["(?:(?:first|last|full)\\s+)?name"]
        },
        "commonFields": {
          "score": 10,
          "tags": "label",
          "matches": ["work (?:history|experience)", "cover letter"]
        },
        "linkedInField": {
          "score": 5,
          "tags": "label",
          "matches": ["linkedin (?:profile|url)"]
        },
        "resumeUploadMatch": {
          "score": 10,
          "tags": "label,strong,button",
          "matches": ["\\b(?:resume|cv)\\b"]
        },
        "goodSubmitBtn": {
          "score": 10,
          "tags": "button, input[type='button']",
          "matches": ["(send|submit)\\s+((your)\\s+)?application"]
        },
        "okaySubmitBtn": {
          "score": 5,
          "tags": "button, input[type='button']",
          "matches": ["apply", "(?:save(?: and continue)?|continue)", "next"]
        }
      }
    },
